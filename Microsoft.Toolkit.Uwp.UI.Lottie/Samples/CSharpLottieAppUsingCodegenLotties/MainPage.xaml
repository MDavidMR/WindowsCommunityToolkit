<Page
    x:Class="CSharpLottieAppUsingCodegenLotties.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:CSharpLottieAppUsingCodegenLotties"
    xmlns:cp="using:Microsoft.UI.Xaml.Controls.CompositionPlayer"
    xmlns:compositions="using:Compositions"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

        <RelativePanel>
        <cp:CompositionPlayer x:Name="Player1" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignTopWithPanel="True">
            <compositions:BirthdayCake/>
        </cp:CompositionPlayer>
        <cp:CompositionPlayer x:Name="Player2" RelativePanel.AlignRightWithPanel="True" RelativePanel.AlignTopWithPanel="True">
            <compositions:Paint/>
        </cp:CompositionPlayer>
        <cp:CompositionPlayer x:Name="Player3" RelativePanel.AlignLeftWithPanel="True" RelativePanel.AlignBottomWithPanel="True">
            <compositions:LottieLogo2/>
        </cp:CompositionPlayer>
        <cp:CompositionPlayer x:Name="Player4" RelativePanel.AlignRightWithPanel="True" RelativePanel.AlignBottomWithPanel="True">
            <compositions:Gift_popup/>
        </cp:CompositionPlayer>
    </RelativePanel>
</Page>
